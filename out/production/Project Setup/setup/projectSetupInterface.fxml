<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<VBox xmlns:fx="http://javafx.com/fxml/1" alignment="CENTER" spacing="5.0" xmlns="http://javafx.com/javafx/8"
  fx:controller="setup.Controller">
  <padding>
    <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
  </padding>
  <TextField onKeyTyped="#checkText" promptText="Project name" fx:id="projectName"/>
  <ChoiceBox prefWidth="150.0" fx:id="languageSelection"/>
  <TextField onKeyTyped="#checkNumber" promptText="Team number" fx:id="teamNumber"/>
  <ChoiceBox prefWidth="150.0" fx:id="ideSelection"/>
  <Button defaultButton="true" disable="true" mnemonicParsing="false" onAction="#setupProject" text="Download"
    fx:id="download"/>
</VBox>
